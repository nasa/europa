    REGISTER_TYPE_FACTORY(id->getTypeFactoryMgr(),Color, ColorBaseDomain());
    REGISTER_TYPE_FACTORY(id->getTypeFactoryMgr(),Random, RandomBaseDomain());
    // Allocate rules
    new Rule$Foo$pred1$0();
    return id;
  }
  
