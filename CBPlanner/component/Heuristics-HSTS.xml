  <Priority>6000.25</Priority>
  <Preference>
  <Generator>DestinationGenerator</Generator>
  </Preference>
  <VariableSpec>
    <Name>to</Name>
  </VariableSpec>
  <PredicateSpec>
  <PredicateName>Navigator.Going</PredicateName>
  <PredicateParameters>
   <Parameter>
     <Index>0</Index>
     <Value>Loc1</Value>
   </Parameter>
  </PredicateParameters>
  </PredicateSpec>
</ConstrainedVariable>

<ConstrainedVariable>
  <Priority>6000.5</Priority>
  <Preference>
  <ValueOrder>descending</ValueOrder>
  </Preference>
  <VariableSpec>
    <Index>0</Index>
  </VariableSpec>
  <PredicateSpec>
  <PredicateName>Commands.TakeSample</PredicateName>
  <PredicateParameters/>
  </PredicateSpec>
</ConstrainedVariable>

<ConstrainedVariable>
  <Priority>6000.25</Priority>
  <Preference>
  <DomainOrder>
    <Value>Loc4</Value>
    <Value>Loc5</Value>
    <Value>Loc6</Value>
  </DomainOrder>
  </Preference>
  <VariableSpec>
    <Index>0</Index>
  </VariableSpec>
  <PredicateSpec>
  <PredicateName>Instrument.TakeSample</PredicateName>
  <PredicateParameters/>
  </PredicateSpec>
</ConstrainedVariable>

<ConstrainedVariable>
  <Priority>9000</Priority>
  <Preference>
  <ValueOrder>ascending</ValueOrder>
  </Preference>
  <VariableSpec>
    <Name>minutes</Name>
  </VariableSpec>
  <PredicateSpec>
  <PredicateName>Telemetry.Communicate</PredicateName>
  <PredicateParameters/>
  </PredicateSpec>
</ConstrainedVariable>

<ConstrainedVariable>
  <Priority>9000</Priority>
  <Preference>
  <DomainOrder>
    <Value>low</Value>
    <Value>medium</Value>
    <Value>high</Value>
    <Value>medium-high</Value>
    <Value>medium-low</Value>
  </DomainOrder>
  </Preference>
  <VariableSpec>
    <Name>mode</Name>
  </VariableSpec>
  <PredicateSpec>
  <PredicateName>Telemetry.Communicate</PredicateName>
  <PredicateParameters/>
  </PredicateSpec>
</ConstrainedVariable>
</VariableSpecification>

<TokenSpecification>
<Token>
  <Priority>443.7</Priority>
  <PredicateSpec>
  <PredicateName>Navigator.At</PredicateName>
  <PredicateParameters/>
  </PredicateSpec>
  <Master/>
  <DecisionPreference/>
</Token>
<Token>
  <Priority>200.4</Priority>
  <PredicateSpec>
  <PredicateName>Commands.TakeSample</PredicateName>
  <PredicateParameters>
   <Parameter>
     <Index>0</Index>
     <Value>Loc3</Value>
   </Parameter>
  </PredicateParameters>
  </PredicateSpec>
  <Master/>
   <DecisionPreference>
    <StateOrder>
       <State>activate</State>
       <Order>early</Order>
    </StateOrder>
  </DecisionPreference>
</Token>
<Token>
  <Priority>10000.0</Priority>
  <PredicateSpec>
  <PredicateName>Navigator.Going</PredicateName>
  <PredicateParameters>
    <Parameter>
     <Index>0</Index>
     <Value>Loc1</Value>
    </Parameter>
    <Parameter>
     <Index>1</Index>
     <Value>Loc3</Value>
    </Parameter>
  </PredicateParameters>
  </PredicateSpec>
  <Master>
    <Relation>after</Relation>
    <PredicateSpec>
      <PredicateName>Navigator.At</PredicateName>
      <PredicateParameters/>
    </PredicateSpec>
  </Master>
  <DecisionPreference>
    <StateOrder>
       <State>merge</State>
       <Order>early</Order>
    </StateOrder>
    <StateOrder>
       <State>activate</State>
       <Order>minFlexible</Order>
    </StateOrder>
    <State>reject</State>
  </DecisionPreference>
</Token>
</TokenSpecification>

</Heuristics-HSTS>
