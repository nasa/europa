SubDir PLASMA System test ;

if ! $(PLASMA_READY) {

local model ;

for model in
	Rover.nddl basic-model.nddl k9.backtrack.moderate.nddl
	k9.nddl path-filter.nddl
{
  NddlSystemTest nddl-planner-tests : runTest.cc : $(model) : System ;
}

NddlSystemTest nddl-planner-tests : runInitialTest.cc : k9-initial.nddl : System ;

for model in
	Rover-transaction.nddl basic-model-transaction.nddl k9.backtrack.moderate-transaction.nddl
	k9-transaction.nddl path-filter-transaction.nddl
{
  NddlSystemTest nddl-planner-tests : runTransactionTest.cc : $(model) : System ;
}

} # PLASMA_READY
