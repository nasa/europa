#include "NddlWorld.nddl"

class Location {

    predicate NotTracked {
      Location target;
      eq(object, target);
    }

    predicate trackstart {
      Location target;
      eq(object, target);
    }

    predicate Tracked {
      Location target;
      eq(object, target);
    }

    predicate trackstop {
      Location target;
      eq(object, target);
    }
}



class Path {
  Location m_from, m_to;

  Path(Location from, Location to) {
    m_from = from;
    m_to = to;
  }  
}

class Position {  
  predicate At {
    Location location;
  }
  
  predicate navigate {
    Location from;
    Location to;
    neq(from, to);
    eq(duration, 1);
  }  
}

Position::At{
  meets(navigate a);
  eq(location , a.from);
  
  met_by(navigate b);
  eq(location, b.to);
}

    

Position::navigate{

  Path path : {
    eq(path.m_from, from);
    eq(path.m_to, to);
  };

  meets(At a);
  eq(to , a.location);
  
  met_by(At b);
  eq(b.location, from);

}

